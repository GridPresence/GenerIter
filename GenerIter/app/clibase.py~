import pyrobodj.excepts as robox

class CLIBase():

    def __init__(self):
        self.parseArguments()
        self.process()

    def parseArguments(self):
        raise robox.RDJNotImplementedErr("The method parseArguments() is not implemented on this object.")

    def process(self):
        raise robox.RDJNotImplementedErr("The method process() is not implemented on this object.")
